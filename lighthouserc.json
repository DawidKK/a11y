{
  "ci": {
    "collect": {
      "url": [
        "http://127.0.0.1:8080"
      ],
      "startServerCommand": "http-server ./build/client -p 8080 -g",
      "startServerReadyPattern": "Available on",
      "numberOfRuns": 1
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "assert": {
      "preset": "lighthouse:recommended"
    }
  }
}
